{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop:gatsby": "gatsby develop",
    "develop:type-check": "tsc --watch",
    "develop": "npm-run-all --parallel develop:gatsby develop:type-check",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@types/react-paginate": "^7.1.1",
    "framer-motion": "^7.6.5",
    "gatsby": "^5.3.3",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-image": "^3.3.2",
    "gatsby-plugin-sharp": "^5.3.2",
    "gatsby-remark-images": "^7.3.1",
    "gatsby-remark-prismjs": "^7.3.0",
    "gatsby-source-filesystem": "^5.3.1",
    "gatsby-transformer-remark": "^6.3.2",
    "gatsby-transformer-sharp": "^5.3.1",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.29.0",
    "react": "^18.1.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.6.0",
    "react-paginate": "^8.1.3",
    "react-waypoint": "^10.3.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "autoprefixer": "^10.4.12",
    "gatsby-plugin-postcss": "^6.3.0",
    "gatsby-remark-static-images": "^1.2.1",
    "postcss": "^8.4.18",
    "postcss-import": "^15.0.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
